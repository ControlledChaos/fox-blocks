{
	"name": "fox-blocks",
	"version": "1.0.0",
	"description": "Fox Blocks",
	"main": "src/index.js",
	"private": true,
	"scripts": {
		"start": "parcel watch src/index.js src/main.css src/editor.css",
		"build": "parcel build src/index.js src/main.css src/editor.css",
		"lint": "npm run lint:css && npm run lint:js && npm run lint:php",
		"lint:css": "cross-env stylelint './src/**/*.css'",
		"lint:js": "cross-env eslint './src/**/*.js'",
		"lint:php": "cross-env ./vendor/bin/phpcs .",
		"package": "./bin/build-plugin-zip.sh"
	},
	"keywords": [
		"wordpress",
		"plugin",
		"javascript",
		"gutenberg",
		"editor"
	],
	"author": "Sami Keijonen",
	"license": "MIT",
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@foxland/stylelint-config": "^1.0.3",
		"@wordpress/babel-plugin-makepot": "^2.1.3",
		"@wordpress/babel-preset-default": "^3.0.2",
		"@wordpress/browserslist-config": "^2.2.3",
		"@wordpress/eslint-plugin": "^1.0.1",
		"babel-plugin-transform-react-jsx": "^6.24.1",
		"cross-env": "^5.2.0",
		"eslint": "^5.13.0",
		"eslint-plugin-jest": "^22.2.2",
		"eslint-plugin-jsx-a11y": "^6.2.1",
		"eslint-plugin-react": "^7.12.4",
		"parcel-bundler": "^1.11.0",
		"postcss-editor-styles": "^0.1.4"
	},
	"browserslist": [
		"extends @wordpress/browserslist-config"
	],
	"dependencies": {
		"classnames": "^2.2.6"
	}
}
